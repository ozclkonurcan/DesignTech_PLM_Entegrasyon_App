@model string[]
@using System.IO
<div class="d-flex flex-wrap flex-stack mb-6">
	<!--begin::Title-->
	<h3 class="fw-bold my-2">
		Belgelerim
		<span class="fs-6 text-gray-400 fw-semibold ms-1">@ViewBag.fileCount excel dosyası</span>
	</h3>
	<!--end::Title-->
	<!--begin::Controls-->
	<div class="d-flex my-2">

		<form method="get" action="/PlmUpdateStokCode/Index">
			<div class="input-group">

				<div class="position-relative w-md-400px me-md-2">
					<i class="ki-duotone ki-magnifier fs-3 text-gray-500 position-absolute top-50 translate-middle ms-6"><span class="path1"></span><span class="path2"></span></i>
					<input type="text" class="form-control form-control-solid ps-10" name="search" value="" placeholder="Dosya Ara...">
				</div>
				<div class="d-flex align-items-center">
					<button type="submit" class="btn btn-primary fs-6 px-8 py-4 me-5">Ara</button>

				</div>
			</div>
		</form>


		<button hidden href="#" class="btn btn-primary er fs-6 px-8 py-4 me-5" data-bs-toggle="modal" data-bs-target="#kt_modal_users_search" disabled>
			Search Excel File
		</button>
		<button  type="button" class="btn btn-flex btn-primary fs-6 px-8 py-4 me-5" data-bs-toggle="modal" data-bs-target="#kt_modal_upload">
			<i class="ki-duotone ki-folder-up fs-2"><span class="path1"></span><span class="path2"></span></i>Excel Dosyası Yükle
		</button>
	</div>
	<!--end::Controls-->
</div>


<div class="row g-6 g-xl-9 mb-6 mb-xl-9">


	<!--begin::Col-->
	<div id="folderList" class="row g-6 g-xl-9 mb-6 mb-xl-9 d-flex align-items-start flex-row">
		@foreach (var item in ViewBag.filelist)
		{
			var fileInfo = item.Split('_');

			if (fileInfo.Length >= 2)
			{
				<div class="col-md-2">
					<div class="card h-100" style="min-height:35vh">
						<div class="card-body d-flex justify-content-center text-center flex-column p-8">
							<form method="post" asp-action="DeleteFile" asp-controller="PlmUpdateStokCode">
								<input type="hidden" name="fileName" value="@item" />

								<button type="submit" class="btn btn-icon bg-danger bg-opacity-10 btn-color-danger btn-active-danger w-35px h-35px text-end float-end">
									<i class="ki-duotone ki-trash-square fs-2">
										<span class="path1"></span>
										<span class="path2"></span>
										<span class="path3"></span>
										<span class="path4"></span>
									</i>
								</button>
							</form>

							<a href="javascript:void(0);" onclick="confirmIntegrate('@item')" class="text-gray-800 text-hover-primary d-flex flex-column">
								<div class="symbol symbol-75px mb-5">
									<img src="/assets/media/svg/files/excelSvf.svg" class="theme-light-show" alt="">
									<img src="/assets/media/svg/files/excelSvf.svg" class="theme-dark-show" alt="">
								</div>
								<div class="fs-5 fw-bold mb-2">
									@fileInfo[0]
								</div>
							</a>
							<div class="fs-7 fw-semibold text-gray-400">
								<span class="badge badge-light-primary">@System.IO.Path.GetFileNameWithoutExtension(fileInfo[1])</span>
							</div>
						</div>
					</div>
				</div>
			}
			else
			{
				<div class="col-md-6 col-lg-4 col-xl-3">
					<h4 class="alert alert-warning">
						Invalid file format
					</h4>
				</div>
			}
		}
	</div>

	<div class="dataTables_paginate paging_simple_numbers" id="kt_ecommerce_products_table_paginate">
		<ul class="pagination">
			@if (ViewBag.page > 1)
			{
				<li class="paginate_button page-item">
					<a href="?page=1" aria-controls="kt_ecommerce_products_table" data-dt-idx="0" tabindex="0" class="page-link">
						<i class="previous"></i>
						<i class="previous"></i>
					</a>
				</li>
				<li class="paginate_button page-item">
					<a href="?page=@(ViewBag.page - 1)" aria-controls="kt_ecommerce_products_table" data-dt-idx="1" tabindex="0" class="page-link">
						<i class="previous"></i>
					</a>
				</li>
			}

			@for (var i = 1; i <= Math.Ceiling((double)ViewBag.fileCount / ViewBag.pageSize); i++)
			{
				if (i == ViewBag.page)
				{
					<li class="paginate_button page-item active">
						<a href="?page=@i" aria-controls="kt_ecommerce_products_table" data-dt-idx="@i" tabindex="0" class="page-link">@i</a>
					</li>
				}
				else if (i == 1 || i == ViewBag.page - 1 || i == ViewBag.page || i == ViewBag.page + 1 || i == Math.Ceiling((double)ViewBag.fileCount / ViewBag.pageSize))
				{
					<li class="paginate_button page-item">
						<a href="?page=@i" aria-controls="kt_ecommerce_products_table" data-dt-idx="@i" tabindex="0" class="page-link">@i</a>
					</li>
				}
				else if (i == 2 || i == ViewBag.page - 2 || i == ViewBag.page + 2 || i == Math.Ceiling((double)ViewBag.fileCount / ViewBag.pageSize) - 1)
				{
					<li class="paginate_button page-item">
						<span class="page-link">...</span>
					</li>
				}
			}

			@if (ViewBag.page < Math.Ceiling((double)ViewBag.fileCount / ViewBag.pageSize))
			{
				<li class="paginate_button page-item">
					<a href="?page=@(ViewBag.page + 1)" aria-controls="kt_ecommerce_products_table" data-dt-idx="6" tabindex="0" class="page-link">
						<i class="next"></i>
					</a>
				</li>
				<li class="paginate_button page-item">
					<a href="?page=@(Math.Ceiling((double)ViewBag.fileCount / ViewBag.pageSize))" aria-controls="kt_ecommerce_products_table" data-dt-idx="7" tabindex="0" class="page-link">
						<i class="next"></i>
						<i class="next"></i>
					</a>
				</li>
			}
		</ul>
	</div>



	<!--end::Col-->





</div>


