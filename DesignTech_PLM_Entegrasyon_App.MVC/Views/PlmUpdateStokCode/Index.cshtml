				</div>
				<!--end::Close-->
			</div>
			<!--end::Modal header-->
			<!--begin::Modal body-->
			<div class="modal-body pt-10 pb-15 px-lg-17">
				<!--begin::Input group-->
				<!--begin::Form-->
				<form class="form" asp-action="ExcelUpload" asp-controller="PlmUpdateStokCode" method="post" enctype="multipart/form-data">
				
					<input name="formFile" type="file" id="fileInput" onchange="validateFile()" />
					<button type="submit" class="btn btn-primary" id="submitButton" disabled>Ekle</button>
			
				</form>
				<!--end::Form-->
				<!--end::Input group-->
